{
	"name": "vnerdtree",
	"version": "0.0.1",
	"description": "A minimal view of NERDTree for VSCode",
	"icon": "icon.png",
	"keywords": [
		"netrw",
		"vim",
		"vim-vinegar",
		"dired",
		"explorer",
		"nerdtree"
	],
	"main": "extension.js",
	"license": "UNLICENSE",
	"engines": {
		"vscode": "^1.80.0"
	},
	"scripts": {
		"build": "tsc && vsce package",
		"install": "yarn build && code --install-extension *.vsix"
	},
	"activationEvents": [
		"onCommand:vnerdtree.open"
	],
	"contributes": {
		"languages": [
			{
				"id": "vnerdtree",
				"filenames": [
					"vnerdtree"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"commands": [
			{
				"command": "vnerdtree.open",
				"title": "Open Explorer",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.toggleFullPaths",
				"title": "Toggle Full Paths",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.toggleBookmarks",
				"title": "Toggle Bookmarks",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.toggleHelp",
				"title": "Toggle Help",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.toggleHidden",
				"title": "Toggle Hidden Files",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.openInitial",
				"title": "Open Initial Directory",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.revealInFileManager",
				"title": "Reveal in File Manager",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.addBookmark",
				"title": "Add Bookmark",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.deleteBookmark",
				"title": "Delete Bookmark",
				"category": "vnerdtree"
			},
			{
				"command": "vnerdtree.jumpToBookmark",
				"title": "Jump to Bookmark",
				"category": "vnerdtree"
			}
		],
		"keybindings": [
			{
				"command": "vnerdtree.open",
				"key": "-",
				"when": "!activeEditor && !inputFocus && !inputBoxFocus && !searchViewletFocus"
			},
			{
				"command": "vnerdtree.openAtCursor",
				"key": "enter",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.openParent",
				"key": "backspace",
				"when": "editorTextFocus && editorLangId == vnerdtree"
			},
			{
				"command": "vnerdtree.close",
				"key": "-",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.openHome",
				"key": "shift+`",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.rename",
				"key": "shift+r",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.delete",
				"key": "shift+d",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.delete",
				"key": "delete",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.create",
				"key": "shift+5",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.createDir",
				"key": "d",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.refresh",
				"key": "ctrl+l",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.toggleFullPaths",
				"key": "shift+p",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.openInitial",
				"key": ".",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.revealInFileManager",
				"key": "o",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.addBookmark",
				"key": "m",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.deleteBookmark",
				"key": "shift+m",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.jumpToBookmark",
				"key": "b",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.toggleBookmarks",
				"key": "shift+b",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.toggleHelp",
				"key": "shift+h",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			},
			{
				"command": "vnerdtree.toggleHidden",
				"key": "i",
				"when": "editorTextFocus && editorLangId == vnerdtree && vim.mode =~ /^(?!SearchInProgressMode|CommandlineInProgress).*$/"
			}
		],
		"grammars": [
			{
				"language": "vnerdtree",
				"scopeName": "source.vnerdtree",
				"path": "./vnerdtree.tmLanguage.json"
			}
		],
		"configurationDefaults": {
			"[vnerdtree]": {
				"editor.lineNumbers": "off",
				"editor.minimap.enabled": false,
				"editor.renderWhitespace": "none"
			}
		}
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@types/vscode": "^1.68.1",
		"typescript": "^4.7.4",
		"vsce": "^2.9.2"
	}
}